MERGE
  {{ dataset }}.{{ main_table }} M
USING
  {{ dataset }}.{{ sharded_table }} S
ON
  M.{{ merge_condition_field }} = S.{{ merge_condition_field }}
WHEN MATCHED AND M.{{ updated_date_field }} <= S.{{ updated_date_field }} OR M.{{ updated_date_field }} is null THEN
  DELETE