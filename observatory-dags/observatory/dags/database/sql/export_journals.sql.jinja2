SELECT
  id,
  name,
  country,
  country_code,
  region,
  subregion,
  coordinates,
  DATE(time_period, 12, 31) AS published_year,
  journal.journal,
  journal.total_outputs,
  journal.num_oa_outputs,
  journal.num_green_outputs,
  journal.num_gold_outputs
FROM
  `{{ project_id }}.{{ dataset_id }}.{{ table_name }}`,
  UNNEST(journals) as journal
ORDER BY id, published_year ASC